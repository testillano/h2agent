{
  "requestMethod": "GET",
  "requestUri": "/app/v1/foo/bar/1",
  "responseCode": 200,
  "responseBody": {
    "foo": "bar-1"
  },
  "responseHeaders": {
    "content-type": "text/html",
    "x-version": "1.0.0"
  },
  "transform": [
    {
      "source": "request.uri",
      "target": "response.body.string./captureBarIdFromURIAndAppendSuffix",
      "filter": {
        "RegexReplace": {
          "rgx": "/app/v1/foo/bar/([0-9]*)",
          "fmt": "$1-suffix"
        }
      }
    }
  ]
}
