[
  {
    "requestMethod": "POST",
    "requestUri": "(/assistant/)([0-9]{5})(/entrena)",
    "responseCode": 201,
    "transform": [
      {
        "source": "request.uri.path",
        "target": "var.ericsson-id",
        "filter": {
          "RegexReplace": {
            "rgx": "(/assistant/)([0-9]{5})(/entrena)",
            "fmt": "$2"
          }
        }
      },
      {
        "source": "command./vagrant/home-projects/GITHUB/h2agent.master/HACKATHON/entrena_modelo.sh @{ericsson-id}",
        "target": "response.body.string"
      }
    ]
  },
  {
    "requestMethod": "POST",
    "requestUri": "(/assistant/)([0-9]{5})(/update)",
    "responseCode": 201,
    "transform": [
      {
        "source": "request.uri.path",
        "target": "var.ericsson-id",
        "filter": {
          "RegexReplace": {
            "rgx": "(/assistant/)([0-9]{5})(/update)",
            "fmt": "$2"
          }
        }
      },
      {
        "source": "request.body./stars",
        "target": "var.stars"
      },
      {
        "source": "var.stars",
        "target": "txtFile./vagrant/home-projects/GITHUB/h2agent.master/HACKATHON/Assistants/@{ericsson-id}.stars"
      }
    ]
  },
  {
    "requestMethod": "GET",
    "requestUri": "(/assistant/)([0-9]{5})(/stars)",
    "responseCode": 200,
    "transform": [
      {
        "source": "request.uri.path",
        "target": "var.ericsson-id",
        "filter": {
          "RegexReplace": {
            "rgx": "(/assistant/)([0-9]{5})(/stars)",
            "fmt": "$2"
          }
        }
      },
      {
        "source": "txtFile./vagrant/home-projects/GITHUB/h2agent.master/HACKATHON/Assistants/@{ericsson-id}.stars",
        "target": "response.body.string"
      }
    ]
  }
]
